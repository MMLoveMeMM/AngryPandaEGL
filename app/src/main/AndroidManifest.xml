        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".egl.p1.TextureviewtestActivity" />
        <activity android:name=".egl.p2.GLTextureActivity" />
        <activity android:name=".egl.p3.BitmapActivity" />
        <activity android:name=".egl.p3.PBufferActivity" />
        <activity android:name=".egl.p4.EglActivity"></activity>
    </application>

</manifest>